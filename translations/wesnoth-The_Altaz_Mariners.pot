#. [message]: speaker=narrator
#: The_Altaz_Mariners/macros/buildings.cfg:267
#: The_Altaz_Mariners/macros/buildings.cfg:292
#: The_Altaz_Mariners/macros/buildings.cfg:322
#: The_Altaz_Mariners/macros/buildings.cfg:356
#: The_Altaz_Mariners/macros/buildings.cfg:367
#: The_Altaz_Mariners/macros/buildings.cfg:378
msgid "Barmaid"
msgstr ""

#. [message]: speaker=narrator
#: The_Altaz_Mariners/macros/buildings.cfg:268
msgid "Didn't you see the sign? No dogs allowed!"
msgstr ""

#. [message]: speaker=narrator
#: The_Altaz_Mariners/macros/buildings.cfg:293
msgid "Look at the state of this one! You've had one too many already, ain't you darling?"
msgstr ""

#. [message]: speaker=narrator
#: The_Altaz_Mariners/macros/buildings.cfg:323
msgid "We're closed now, love. You drunk us dry!"
msgstr ""

#. [message]: speaker=narrator
#: The_Altaz_Mariners/macros/buildings.cfg:368
msgid "Hello sailor!"
msgstr ""

#. [message]: speaker=narrator
#: The_Altaz_Mariners/macros/buildings.cfg:379
msgid "Oh, you do look thirsty. I'll be right over..."
msgstr ""

#. [option]
#: The_Altaz_Mariners/macros/buildings.cfg:445
msgid "Leave Tavern"
msgstr ""

