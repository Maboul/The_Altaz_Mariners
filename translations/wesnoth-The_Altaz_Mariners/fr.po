#: The_Altaz_Mariners/units/humans/Cut-Throat.cfg:4
#: The_Altaz_Mariners/units/humans/Cut-Throat.cfg:19
msgid "Cut-Throat"
msgstr "Gorge coupée"

#. [unit_type]: id=TAM_Druid , race=human
#: The_Altaz_Mariners/units/humans/Druid.cfg:4
msgid "Druid"
msgstr "Druide"

#. [unit_type]: id=TAM_Privateer, race=human
#: The_Altaz_Mariners/units/humans/Seafarer_Privateer.cfg:4
msgid "Privateer"
msgstr "Corsaire"

#. [unit_type]: id=TAM_Privateer, race=human
